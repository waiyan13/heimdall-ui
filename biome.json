{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
    "enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": []
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "space"
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
      "nursery": {
        "useSortedClasses": {
          "level": "warn"
        }
      }
		}
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double"
		}
	}
}
